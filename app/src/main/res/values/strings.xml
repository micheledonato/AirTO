<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <string name="app_name" translatable="false">AirTO</string>

    <string name="content_authority" translatable="false">com.devmicheledonato.airto</string>

    <string name="action_settings">Settings</string>
    <string name="today">Today</string>
    <string name="tomorrow">Tomorrow</string>
    <string name="no_network_no_weather_data">No network connectivity!</string>
    <string name="no_network">Will refresh when network available.</string>
    <string name="error_data">No weather data&#8230;\nWill load data when the network is available.</string>

    <!-- Temperature format -->
    <string name="format_temperature" translatable="false">
        <xliff:g id="temp">%1.0f</xliff:g>\u00B0
    </string>

    <!-- The format of the notification we show when the weather is updated -->
    <string name="format_notification">
        Forecast:<xliff:g id="condition">%1$s</xliff:g>
        High:<xliff:g id="high">%2$s</xliff:g>
        Low:<xliff:g id="low">%3$s</xliff:g>
        <xliff:g id="ipqa">%4$s</xliff:g>
    </string>

</resources>
